<div style="margin-top:1em;">
	<label>按钮(bootstrap用法)</label><br>
	<button type="button" class="btn btn-default">Default</button>
	<button type="button" class="btn btn-primary">Primary</button>
	<button type="button" class="btn btn-success">Success</button>
	<button type="button" class="btn btn-info">Info</button>
	<button type="button" class="btn btn-warning">Warning</button>
	<button type="button" class="btn btn-danger">Danger</button>
	<button type="button" class="btn btn-inverse">Inverse</button>
	<button type="button" class="btn btn-gray">Gray</button><br /><br />
	<button type="button" class="btn btn-default-outline">Default</button>
	<button type="button" class="btn btn-primary-outline">Primary</button>
	<button type="button" class="btn btn-success-outline">Success</button>
	<button type="button" class="btn btn-info-outline">Info</button>
	<button type="button" class="btn btn-warning-outline">Warning</button>
	<button type="button" class="btn btn-danger-outline">Danger</button>
	<button type="button" class="btn btn-inverse-outline">Inverse</button>
	<button type="button" class="btn btn-gray-outline">Gray</button>
	<button type="button" class="btn btn-link">Link</button>
</div>

<div style="margin-top:1em;">
	<label>按钮(easyui用法)</label><br>
	<a class="bsui-linkbutton" data-options="btnCls:'default'">Default</a>
	<a class="bsui-linkbutton" data-options="btnCls:'primary'">Primary</a>
	<a class="bsui-linkbutton" data-options="btnCls:'success'">Success</a>
	<a class="bsui-linkbutton" data-options="btnCls:'info'">Info</a>
	<a class="bsui-linkbutton" data-options="btnCls:'warning'">Warning</a>
	<a class="bsui-linkbutton" data-options="btnCls:'danger'">Danger</a>
	<a class="bsui-linkbutton" data-options="btnCls:'inverse'">Inverse</button>
	<a class="bsui-linkbutton" data-options="btnCls:'gray'" style="margin-left: .3em;">Gray</a><br /><br />
	<a class="bsui-linkbutton" data-options="btnCls:'default-outline'">Default</button>
	<a class="bsui-linkbutton" data-options="btnCls:'primary-outline'" style="margin-left: .3em;">Primary</button>
	<a class="bsui-linkbutton" data-options="btnCls:'success-outline'" style="margin-left: .3em;">Success</button>
	<a class="bsui-linkbutton" data-options="btnCls:'info-outline'" style="margin-left: .3em;">Info</button>
	<a class="bsui-linkbutton" data-options="btnCls:'warning-outline'" style="margin-left: .3em;">Warning</button>
	<a class="bsui-linkbutton" data-options="btnCls:'danger-outline'" style="margin-left: .3em;">Danger</button>
	<a class="bsui-linkbutton" data-options="btnCls:'inverse-outline'" style="margin-left: .3em;">Inverse</a>
	<a class="bsui-linkbutton" data-options="btnCls:'gray-outline'" style="margin-left: .3em;">
	<a class="bsui-linkbutton" data-options="btnCls:'link'" style="margin-left: .3em;">Link</button>
</div>

